<table class="table">
    <tr class="thead-dark">
        <th scope="col">#</th>
        <th scope="col">Doctor's Name</th>
        <th scope="col">Specialization</th>
        
        <th>Operations</th>
    </tr>

    <% if(data.length){ for(var i=0; i< data.length; i++) {%>
        <tr>
            <td scope="row">
                <%=data[i].id%>
            </td>
            <td><a href="/doctors/<%=data[i].id%>">
                    <%= data[i].name %>
                </a></td>

            <td>
                <%= data[i].specialization%>
            </td>
            <td>
                <a href="/updatedoc/<%=data[i].id%>">update</a>
                <a href="/deletedoc/<%=data[i].id%>">delete</a>
            </td>
        </tr>
        <% } }else{ %>
            <tr>
                <td colspan="3">No Doctors</td>
            </tr>
            <% } %>
</table>
</div>

<a href="/adddoc">ADD</a>
